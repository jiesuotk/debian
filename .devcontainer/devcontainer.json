{
    "name": "Debian 11 Codespace",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "."
    },
    "remoteUser": "root",
    "features": {},
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go",
                "ms-vscode.node-debug2",
                "esbenp.prettier-vscode"
            ]
        }
    },
    "postCreateCommand": "go version && node -v && pnpm -v"
}
